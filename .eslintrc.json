{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:all",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return":"off",
        "indent": [
            "error"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "padded-blocks":"off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "sort-vars": "off",
        "sort-keys": "off"
    }
}